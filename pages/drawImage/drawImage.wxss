page {
  height: 100%;
}

.container {
  position: relative;
  padding: 0rpx;
  width: 100%;
  height: 100%;
}

.canvas_area {
  width: 100%;
  height: calc(100% - 50px);
  background-color: #f8f8f8;
  position: relative;
}

.myCanvas {
  width: 100%;
  height: 100%;
  background: #fff;
}

.tools {
  width: 100%;
  height: 50px;
  padding: 4px 0;
  background: white;
  border-top: 1px solid pink;
}

.canvas_tools>view {
  position: relative;
  width: 100%;
  height: 40px;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}

.box {
  width: 40px;
  height: 40px;
  border-radius: 50%;
  background-color: rebeccapurple;
  box-shadow: rgba(0, 0, 0, 0.2) 2px 2px 5px;
  
}

